{
  "name": "Account",
  "plural": "accounts",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "fullname": {
      "type": "string"
    },
    "avatarUrl": {
      "type": "string"
    },
    "username": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "onlineStatus": {
      "type": "string",
      "required": true,
      "enum": [
        "active",
        "inactive"
      ],
      "default": "inactive"
    },
    "email": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "debtDetails": {
      "type": "hasMany",
      "model": "DebtDetail",
      "foreignKey": "debtId",
      "through": "Bill"
    },
    "notifications": {
      "type": "hasMany",
      "model": "Notification",
      "foreignKey": ""
    },
    "friendship": {
      "type": "hasAndBelongsToMany",
      "model": "Friendship",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}

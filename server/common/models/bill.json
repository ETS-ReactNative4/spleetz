{
  "name": "Bill",
  "plural": "bills",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "date": {
      "type": "date",
      "defaultFn": "now"
    },
    "location": {
      "type": "string"
    },
    "items": {
      "type": [
        {
          "id": "number",
          "name": "string",
          "price": "number",
          "quantity": "number",
          "borrower": []
        }
      ]
    },
    "category": {
      "type": "string",
      "default": "food"
    }
  },
  "validations": [],
  "relations": {
    "payer": {
      "type": "belongsTo",
      "model": "Account",
      "foreignKey": "payerId"
    },
    "borrower": {
      "type": "hasAndBelongsToMany",
      "model": "Account",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}

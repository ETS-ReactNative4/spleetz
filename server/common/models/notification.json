{
  "name": "Notification",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "content": {
      "type": "string"
    },
    "status": {
      "type": "string",
      "required": true,
      "enum": [
        "pending",
        "rejected",
        "accepted"
      ],
      "default": "pending"
    },
    "createdAt": {
      "type": "string", 
      "dataType": "date",
      "required": true,
      "defaultFn": "now"
    },
    "checkedAt": {
      "type": "string", 
      "dataType": "date"
    },
    "price": {
      "type": "number"
    },
    "type": {
      "type": "string",
      "required": true,
      "enum": [
        "other",
        "request",
        "pay"
      ],
      "default": "other"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
